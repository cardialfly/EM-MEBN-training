      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>target/</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>*.jar</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>models/</directory>
      <outputDirectory>models</outputDirectory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
  </fileSets>
